{
  "name": "memory-gpt",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "develop:backend": "yarn --cwd backend start",
    "develop:frontend": "yarn --cwd frontend start",
    "develop": "cross-env FORCE_COLOR=1 npm-run-all -l -p develop:*",
    "start": "yarn develop",
    "preinstall": "cd frontend && yarn install",
    "postinstall": "cd backend && yarn install"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.19"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5"
  }
}
